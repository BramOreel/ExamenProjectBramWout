+package RPG;
+
+import java.util.UUID;
+
+/**
+ * An abstract class of equipable items of heroes and monsters
+ *
+ * @invar Each equipable item must have a unique identification number.
+ *        | canHaveAsId(getId())
+ */
+public abstract class Equipable {
+
+
+    /**
+     * Variable listing the Identification Number of the item.
+     */
+    private long Id;
+
+
+
+    protected Equipable(){
+
+    }
+
+
+    public long getId() {
+        return Id;
+    }
+
+    protected final void setId(long id) {
+        Id = id;
+
+
+    }
+
+    protected boolean canHaveAsId(long id){
+       if(id >= 0 && id < Integer.MAX_VALUE)
+       return true;
+       else return false;
+    }
+}
+
+
+
